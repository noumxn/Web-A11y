{
  "name": "accessibility-testing-tool",
  "module": "app.js",
  "type": "module",
  "contributors": [
    "Ashna Razdan",
    "Cindy Lee",
    "Zuting Chen",
    "Nouman Syed"
  ],
  "devDependencies": {
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "prettier": "^3.1.0"
  },
  "scripts": {
    "test": "mocha --require esm tests/**/*.test.js",
    "start": "node app.js",
    "lint": "eslint --ext .ts,.tsx ./src",
    "fix": "yarn format && yarn lint:fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "fix:eslint": "eslint src --ext .js,.ts,.jsx,.tsx --fix",
    "fix:prettier": "prettier --write ."
  },
  "dependencies": {
    "axios": "^1.5.1",
    "chalk": "^5.3.0",
    "esm": "^3.2.25",
    "fs-extra": "^11.1.1",
    "jsdom": "^22.1.0",
    "text-readability": "^1.0.5"
  }
}
